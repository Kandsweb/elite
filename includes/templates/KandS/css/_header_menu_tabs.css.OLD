#opaque_all{
  position: absolute;
  left: 0;
  top: 0;
  background: #FFF;
  display: none;
  width: 100%;
  height: 100%;
  z-index: 2000;
  filter: alpha(opacity=40);
  opacity: 0.4;
}

.tabsRoot{
/*border-bottom: 1px solid gray;*/
text-decoration: none;
background-color: black;
padding: 5px 0px 0px 0px;
height: 58px;
}

.tabsRoot ul{
padding: 3px 0;
margin-left: 0;
margin-top: 0px;
margin-bottom: 0;
font: 14px Verdana;
list-style-type: none;
text-align: center; /*set to left, center, or right to align the menu as desired*/

}

#cat2 a{
  background-color: blue;
}

.tabsRoot li{
display: inline;
margin: 0;
}

.tabsRoot li a{
text-decoration: none;
padding: 5px 210px;
margin-right: 0px;
/*border: 1px solid #778;
background: black url(media/bluetab.gif) top left repeat-x; /*THEME CHANGE HERE*/
color: white;
background-color: gray;
}

.tabsRoot li a:hover{
text-decoration: none;
color: #2d2b2b;
}

.tabsRoot li a:visited{
color: white;
}

.tabsRoot li.selected{
  color: yellow;
}

.tabsRoot li.selected a{ /*selected main tab style */
/*background-image: url(media/bluetabactive.gif); THEME CHANGE HERE*/
border-bottom-color: white;
}

.tabsRoot li.selected a:hover{ /*selected main tab style */
text-decoration: none;
}

/* ######### Style for Sub Cats ######### */
.tabsSub{
  background-color: gray;
  padding: 5px,0px;
  margin: 3px 19px;
  height: 27px;
}

.tabsSub ul{
  padding: 3px 0;
  margin-left: 0;
  margin-top: -1px;
  margin-bottom: 0;
  font: 13px Verdana;
  list-style-type: none;
  text-align: center; /*set to left, center, or right to align the menu as desired*/
}

.tabsSub  li a{
text-decoration: none;
padding: 0px 5px;
margin: 0px;
border: 1px solid #778;
/*background: black url(media/bluetab.gif) top left repeat-x; /*THEME CHANGE HERE*/
color: white;
background-color: gray;
line-height: 15px;
}

/* ######### Style for Drop Down Menu ######### */

.dropSub1{
position:absolute;
top: 0;
border: 1px solid #918d8d; /*THEME CHANGE HERE*/
border-width: 0 1px;
font:normal 12px Verdana;
line-height:18px;
z-index:2000;
background-color: white;
width: 200px;
visibility: hidden;
}


.dropSub1 a{
width: auto;
display: block;
text-indent: 5px;
border: 0 solid #918d8d; /*THEME CHANGE HERE*/
border-bottom-width: 1px;
padding: 2px 0;
text-decoration: none;
color: black;
}

* html .dropSub1 a{ /*IE only hack*/
width: 100%;
}

.dropSub1 a:hover{ /*THEME CHANGE HERE*/
background-color: #edf3f3;
}

























.tabSub1{
position:absolute;
top: 0;
border: 1px solid #918d8d; /*THEME CHANGE HERE*/
border-width: 0 1px;
font:normal 12px Verdana;
line-height:18px;
z-index:1100;
background-color: white;

visibility: hidden;
text-align: left;
}


.tabSub1 a{
width: auto;
display: block;
text-indent: 5px;
border: 0 solid #918d8d; /*THEME CHANGE HERE*/
border-bottom-width: 1px;
padding: 2px 5px;
text-decoration: none;
color: black;
}

* html .tabSub1 a{ /*IE only hack*/
width: 100%;
}

.tabSub1 a:hover{ /*THEME CHANGE HERE*/
background-color: #edf3f3;
}
