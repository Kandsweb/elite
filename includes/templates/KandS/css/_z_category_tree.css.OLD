@charset "utf-8";
/* CSS Document */

#simplecategoriestree{
	margin:10px;
	background-color:#03F;
	width:240px;
}

body {  
	behavior: url(includes/csshover.htc);
	}
.simple_tree_wrapper{
	width:220px;
	margin:0 auto;
  text-align:center;
	z-index:1000;
	position:relative;
	height:350px;

	background-color:#EEE;
}

/*#simplecategoriestree a{
	text-decoration:none;
}
*/
.simple_tree_wrapper ul {
	margin:0px;
	padding:0px;
	}
	
.simple_tree_wrapper li {
/*	position:relative;
	list-style:none; 
*/	margin:0px; 
	line-height:1.8em;
	width:550px;
	*width:550px;
	}


.simple_tree_wrapper li a {
	display: block;
	/* Hides from IE5-mac \*/
	height: 1%;
	/* End hide from IE5-mac */ 
	padding: .6em 0px; 
	text-decoration: none; 
	text-transform:uppercase; 
	color:#000;
	font-size:14px;
	text-align:left; 
	border-right:1px solid #ffffff;
}	

.simple_tree_wrapper ul.level1 {
	width:220px;
	margin:0 auto; 
	text-align:center;
	background:#EEE; /*background color of top menu when NOT selected. */
	z-index:1000;
	float:left;
	list-style:none;
	}

.simple_tree_wrapper li:hover {}

.simple_tree_wrapper li.submenu:hover {}

.simple_tree_wrapper li a.noLine {
	border:none;
}

.simple_tree_wrapper ul a {
	width: 220px;
	z-index:1000;
}

.simple_tree_wrapper ul ul {
	display: none;
}

.simple_tree_wrapper ul ul li {
	border-bottom: 1px solid #CCC; 
	width:100%;
	z-index:1000;
	margin: 0px 0px 0px 0px;
}

.simple_tree_wrapper ul.level2{
	position: absolute; 
	width: 220px;
	display:block;
	z-index:1000;
	list-style:none;
	left:0px;
	background:#999;
}

.simple_tree_wrapper ul.level2 {
	z-index:1000;
	position:absolute;
}

.simple_tree_wrapper ul.level2 a {
	padding: 0em 0 0em 0.25em;
	color: black;  /* this is text color on drop-down submenu */
	text-transform:none;
	} 

.simple_tree_wrapper ul.level2 a:hover {
	width:215px;
	height:auto;
}

.simple_tree_wrapper ul.level3{
	top: 1.8em; 
	left: 220px; 
	background:#99a;
	/*width:130px;*/
}

.simple_tree_wrapper ul.level3 a:hover {
	width:116px;
}

.simple_tree_wrapper ul.level4, 
.simple_tree_wrapper ul.level5, 
.simple_tree_wrapper ul.level6, 
.simple_tree_wrapper ul.level7,
.simple_tree_wrapper ul.level8{
	top: 1.8em; 
	left: 120px; 
	background:#99a;
	width:130px;
}

.simple_tree_wrapper ul.level2 li.submenu:hover ul.level3,
.simple_tree_wrapper ul.level3 li.submenu:hover ul.level4,
.simple_tree_wrapper ul.level4 li.submenu:hover ul.level5,
.simple_tree_wrapper ul.level5 li.submenu:hover ul.level6,
.simple_tree_wrapper ul.level6 li.submenu:hover ul.level7,
.simple_tree_wrapper ul.level7 li.submenu:hover ul.level8 {
	position: absolute; 
	width: 120px;
	display:block;
	z-index:1000;
	list-style:none;
}

.clearBoth {
	clear:both;
	height:0;
	font-size:0;
	line-height:0;
	}
	
	
